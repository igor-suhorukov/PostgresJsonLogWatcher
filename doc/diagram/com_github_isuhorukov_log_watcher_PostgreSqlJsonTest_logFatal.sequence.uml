@startuml

participant PostgreSqlJsonTest as "PostgreSqlJsonTest"
participant PostgreSqlJson as "PostgreSqlJson"
participant EnrichmentOff as "EnrichmentOff"
  [->PostgreSqlJsonTest: logFatal <color:gray> 0.219 ms</color>
  activate PostgreSqlJsonTest
    PostgreSqlJsonTest->PostgreSqlJson: parseLogLine <color:gray> 0.178 ms</color>
    activate PostgreSqlJson
      Loop 2 times <color:gray> 0.0413 ms</color>
        PostgreSqlJson->EnrichmentOff: enricherApplicationName <color:gray> 0.0413 ms</color>
        activate EnrichmentOff
        PostgreSqlJson<--EnrichmentOff: void
        deactivate EnrichmentOff
      End
      PostgreSqlJson->EnrichmentOff: getStatement <color:gray> 0.00899 ms</color>
      activate EnrichmentOff
      PostgreSqlJson<--EnrichmentOff: void
      deactivate EnrichmentOff
    PostgreSqlJsonTest<--PostgreSqlJson: void
    deactivate PostgreSqlJson
  [<--PostgreSqlJsonTest: void
  deactivate PostgreSqlJsonTest
@enduml